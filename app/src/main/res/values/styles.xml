<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="TN_NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowTranslucentStatus" tools:ignore="NewApi">true</item>
        <item name="android:windowTranslucentNavigation" tools:ignore="NewApi">true</item>
        <item name="colorPrimary">#2e86de</item>
        <item name="colorPrimaryDark">#81ecec</item>
        <item name="colorAccent">#2e86de</item>
    </style>
</resources>
